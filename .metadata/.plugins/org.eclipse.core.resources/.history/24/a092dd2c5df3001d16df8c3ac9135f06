
public class DiarioDeAula {
	private String data;
	private String horaIncial;
	private String horaFinal;
	private String tarefasDeCasa;
	private String feedback;
	private String dificuldades;
	private String ideiasAulasFuturas;
	private boolean pago;
	//private int statusAula;
	//private int materiaisUtilizados;
	
	
	//construtores
	public DiarioDeAula(String data, String horaIncial, String horaFinal, /* int statusAula, */boolean pago) {
		setData(data);
		setHoraIncial(horaIncial);
		setHoraFinal(horaFinal);
		//this.statusAula = statusAula; //pago depende do status da aula
		setPago(false);//a aula sempre começa como não paga, pode ser atualizado no dia do pagamento
	}

	
	//getters & setters
	public String getData() {
		return data;
	}
	
	
	public void setData(String data) {
		this.data = data;
	}
	
	
	public String getHoraIncial() {
		return horaIncial;
	}
	
	
	public void setHoraIncial(String horaIncial) {
		this.horaIncial = horaIncial;
	}
	
	
	public String getHoraFinal() {
		return horaFinal;
	}
	
	
	public void setHoraFinal(String horaFinal) {
		this.horaFinal = horaFinal;
	}
	
	
	public String getTarefasDeCasa() {
		return tarefasDeCasa;
	}
	
	
	public void setTarefasDeCasa(String tarefasDeCasa) {
		this.tarefasDeCasa = tarefasDeCasa;
	}
	
	
	public String getFeedback() {
		return feedback;
	}
	
	
	public void setFeedback(String feedback) {
		this.feedback = feedback;
	}
	
	
	public String getDificuldades() {
		return dificuldades;
	}
	
	
	public void setDificuldades(String dificuldades) {
		this.dificuldades = dificuldades;
	}
	
	
	public String getIdeiasAulasFuturas() {
		return ideiasAulasFuturas;
	}
	
	
	public void setIdeiasAulasFuturas(String ideiasAulasFuturas) {
		this.ideiasAulasFuturas = ideiasAulasFuturas;
	}
	
	
	public boolean isPago() {
		return pago;
	}
	
	
	public void setPago(boolean pago) {
		this.pago = pago;
	}
	
	
	/*
	 * public int getStatusAula() { return statusAula; }
	 * 
	 * 
	 * public void setStatusAula(int statusAula) { this.statusAula = statusAula; }
	 * 
	 * 
	 * public int getMateriaisUtilizados() { return materiaisUtilizados; }
	 * 
	 * 
	 * public void setMateriaisUtilizados(int materiaisUtilizados) {
	 * this.materiaisUtilizados = materiaisUtilizados; }
	 */
	
	
	
}//DiarioDeAula.
